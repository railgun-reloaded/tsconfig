{
  "extends": [
    "@tsconfig/strictest/tsconfig"
  ],
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "esnext",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "incremental": true,
    "inlineSourceMap": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "${configDir}/eslint.config.js"
  ]
}
